<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="EntrenamientoNatacion.UI.DataMedidas"
        Title="DataGrid">
    <DataGrid Name="DataMedidas" AutoGenerateColumns="False">
        <DataGrid.Columns>
            <DataGridTextColumn
                Header="Fecha"
                Binding="{Binding }"/>
            <DataGridTextColumn
                Header="Peso"
                Binding="{Binding }"/>
            <DataGridTextColumn
                Header="Circ abdominal"
                Binding="{Binding }"/>
            <DataGridTextColumn
                Header="Notas"
                Binding="{Binding }"/>
        </DataGrid.Columns>
    </DataGrid>
</Window>
